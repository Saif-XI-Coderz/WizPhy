<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="187.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="wizphy.controller.TheCourseOptionsController">
  <children>
    <VBox id="VBox" alignment="CENTER" layoutX="14.0" layoutY="22.0" spacing="5.0">
      <children>
        <HBox id="HBox" alignment="CENTER" spacing="5.0">
          <children>
            <ImageView id="video.jpg" fitHeight="100.0" fitWidth="100.0">
              <image>
                <Image url="@../img/video.jpg" preserveRatio="true" smooth="true" />
              </image>
            </ImageView>
            <Button mnemonicParsing="false" onAction="#watchVideo" text="Watch" />
          </children>
        </HBox>
        <HBox id="HBox" alignment="CENTER" spacing="5.0">
          <children>
            <ImageView id="notes.png" fitHeight="100.0" fitWidth="100.0">
              <image>
                <Image url="@../img/notes.png" preserveRatio="true" smooth="true" />
              </image>
            </ImageView>
            <Button mnemonicParsing="false" onAction="#readNotes" text="Read" />
          </children>
        </HBox>
        <HBox id="HBox" alignment="CENTER" spacing="5.0">
          <children>
            <ImageView id="quiz.png" fitHeight="100.0" fitWidth="100.0">
              <image>
                <Image url="@../img/quiz.png" preserveRatio="true" smooth="true" />
              </image>
            </ImageView>
            <Button mnemonicParsing="false" onAction="#takeQuiz" text="Test" />
          </children>
        </HBox>
      </children>
    </VBox>
  </children>
</AnchorPane>
